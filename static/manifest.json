{
	"manifest_version": 2,
	"name": "TabCraft",
	"short_name": "TabCraft",
	"version": "0.0.1",
	"description": "A customizable new tab page that works for you.",
	"applications": {
		"gecko": {
			"id": "tabcraft@kablamo.me",
			"strict_min_version": "54.0"
		}
	},
	"chrome_settings_overrides": {
		"homepage": "index.html"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {
		"32": "icons/32.png",
		"48": "icons/48.png",
		"96": "icons/96.png",
		"128": "icons/128.png"
	},
	"permissions": ["storage"]
}
